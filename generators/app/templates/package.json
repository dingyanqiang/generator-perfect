{
  "name": "<%= projectName %>",
  "projectType": "<%= projectType %>",
  "styleType": "<%= styleType%>",
  "version": "<%= version %>",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dll": "webpack --progress --hide-modules --config webpack.dll.js",
    "server": "node webpack.server.js",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --config webpack.prod.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [""],
  "author": "<%= author %>",
  "email": "<%= email %>",
  "license": "ISC",
  "webpackConfig": {
    "entryPage": "src/pages/*",
    "port": "8888",
    "devtool": "eval-source-map",
    "enableDll": true,
    "enableCommon": false,
    "cssModule": true
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chalk": "^2.1.0",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.1.0",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.1",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.30.1",
    <% if (styleType == "scss") { %>
    "node-sass": "^4.0.0",
    "sass-loader": "^6.0.0",
    <% } %>
    <% if (styleType == "less") { %>
    "less": "^2.3.1",
    "less-loader": "^4.0.5",
    <% } %>
    "morgan": "^1.9.0",
    "open-browser-webpack-plugin": "^0.0.5",
    "postcss-loader": "^2.0.8",
    "pug": "^2.0.0-rc.4",
    "style-loader": "^0.19.0",
    "webpack": "^3.7.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "moment": "^2.19.4",
    <% if (projectType == "react") { %>
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
    <% } %>
    <% if (projectType == "vue") { %>
      "vue": "^2.5.0"
    <% } %>
  }
}
